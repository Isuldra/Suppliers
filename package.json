{
  "name": "one-med-supplychain-app",
  "version": "1.0.0",
  "description": "Desktop application for managing OneMed supply chain and vendor relations",
  "main": "dist/main/main.cjs",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development electron-vite dev",
    "dev:no-warnings": "cross-env NODE_NO_WARNINGS=1 NODE_ENV=development electron-vite dev",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc --noEmit",
    "start": "vite",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "pack": "electron-builder --dir",
    "dist": "electron-vite build && electron-builder",
    "clean-dist": "node scripts/clean-build.js",
    "dist:win": "electron-vite build && electron-builder --win",
    "dist:portable": "electron-vite build && electron-builder --win portable",
    "dist:msi": "electron-vite build && electron-builder --win msi",
    "dist:nsis": "electron-vite build && electron-builder --win nsis",
    "portable": "node scripts/create-portable.js",
    "distributions": "node scripts/create-all-distributions.js",
    "release": "electron-vite build && electron-builder --win --publish always",
    "prepare-win-build": "node scripts/prepare-windows-build.js",
    "cross-build-win": "npm run prepare-win-build && npm run dist:portable",
    "build-for-work": "node scripts/build-for-work-pc.js",
    "test-sqlite": "node scripts/test-sqlite-dependencies.js",
    "build-x64-portable": "node scripts/build-x64-portable.js",
    "build-windows-zip": "node scripts/build-windows-zip.js",
    "verify-windows-build": "node scripts/verify-windows-build.js",
    "build-and-verify": "npm run build-windows-zip && npm run verify-windows-build",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "com.onemed.supplychain",
    "productName": "OneMed SupplyChain",
    "copyright": "Copyright Â© 2024 OneMed",
    "asar": true,
    "asarUnpack": [
      "dist/main/database.js",
      "dist/main/databaseAdapter.js",
      "dist/services/databaseServiceAdapter.js",
      "node_modules/better-sqlite3/build/Release/better*.node",
      "node_modules/sqlite3/**"
    ],
    "files": [
      {
        "from": "dist",
        "to": "."
      },
      "package.json"
    ],
    "npmRebuild": false,
    "directories": {
      "buildResources": "resources",
      "output": "release"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "Isuldra",
        "repo": "Suppliers"
      }
    ],
    "win": {
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "resources/icon.ico",
      "publisherName": "OneMed",
      "artifactName": "${productName}-${version}-setup.${ext}"
    },
    "portable": {
      "artifactName": "${productName}-Portable.exe",
      "requestExecutionLevel": "user"
    },
    "msi": {
      "artifactName": "${productName}-${version}-Setup.msi",
      "oneClick": false,
      "perMachine": false,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "OneMed SupplyChain",
      "upgradeCode": "7C04A11D-9FAA-4B83-85FF-79D2AABFD84E"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "allowElevation": false,
      "installerIcon": "resources/icon.ico",
      "uninstallerIcon": "resources/icon.ico",
      "installerHeaderIcon": "resources/icon.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "OneMed SupplyChain",
      "deleteAppDataOnUninstall": false,
      "displayLanguageSelector": false,
      "installerSidebar": "resources/sidebar.bmp",
      "uninstallerSidebar": "resources/sidebar.bmp",
      "include": "resources/installer.nsh"
    },
    "mac": {
      "target": [
        "dmg"
      ],
      "category": "public.app-category.business",
      "icon": "resources/icon.icns"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Office"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@electron/get": "^4.0.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/handlebars": "^4.0.40",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.24",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.4.0",
    "@vitest/coverage-v8": "^1.3.1",
    "adm-zip": "^0.5.16",
    "autoprefixer": "^10.4.18",
    "cross-env": "^7.0.3",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "electron-rebuild": "^3.2.9",
    "electron-vite": "^2.0.0",
    "jsdom": "^24.0.0",
    "png-to-ico": "^2.1.8",
    "postcss": "^8.4.35",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-ignore": "^1.0.10",
    "sharp": "^0.34.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vitest": "^1.3.1"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@tanstack/react-table": "^8.21.3",
    "@types/better-sqlite3": "^7.6.13",
    "@types/react-select": "^5.0.0",
    "@types/xlsx": "^0.0.35",
    "better-sqlite3": "^11.9.1",
    "electron-log": "^5.3.3",
    "electron-updater": "^6.6.2",
    "handlebars": "^4.7.8",
    "nodemailer": "^6.10.1",
    "odbc": "^2.4.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^6.22.1",
    "react-select": "^5.10.1",
    "xlsx": "^0.18.5"
  }
}
